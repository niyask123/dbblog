datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model Blog {
  id          Int      @id @default(autoincrement())
  heading     String
  title       String
  description String
  blogURL     String
  date        DateTime
  image       String
}

model Project {
  id        Int      @id @default(autoincrement())
  heading   String
  caption   String
  languages String
  url       String
  date      DateTime
  images    String[]
}
